{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Debug Tests",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"purpose": ["debug-test"],
			"console": "integratedTerminal",
			"env": { "PYTHONPATH": "${workspaceRoot}" },
			"justMyCode": false,
            "cwd": "${workspaceFolder}"
		}
	]
}
